.back{
    position: relative;
    background: #ffffff;
    margin-bottom: 22rpx;
}
.back:after {
    content:" ";
    position:absolute;
    left:0;
    bottom:0;
    right:0;
    border-bottom: 1px solid #ffffff;
}
