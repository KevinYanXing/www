/*列表*/
.top-slider {
  display: block;
  width: 100%;
  height: 40%;
  background: white;
  border: 2rpx solid #e5e5e5;
}
.slider-sec{
  width: 100%;
  height: 100%;
}
.slide-image{
  width:100%;
  height:100%;
  padding: 22rpx;
}
.list-detail {
  width: 100%;
  background: white;
  margin-bottom: 25rpx;
}
.list-detail .list-sec {
  width: 100%;
  display:inline-block;
  padding: 18rpx 0 18rpx;
}
.background{
  width:100%;
  /*background:#f5f5f5;*/
  background: #fff;
  padding:15rpx 0;
}
.image {
  display:block;
  width:150rpx;
  height:150rpx;
  border:1rpx solid #eaedf4;
  margin:8rpx auto;
  border-radius:0rpx;
  text-align:center;
  float:left;
  background: #ffffff;
}
.image image{
  display:block;
  width:150rpx;
  height:150rpx;
}
.bottom-sec {
  margin-left: 175rpx;
}
.now {
  /*color: #666;*/
  font-size: 14px;
  padding-bottom:6rpx; 
}
.top-title{
  padding-bottom:4rpx;
}
/*.top-title .inline-block, .price-wrapper .inline-block{
    vertical-align:top;
}*/
.top-title .title{
  /*width: 14%;*/
  color: #BBBBBB;
  font-size:26rpx;
}
.top-title .description{
  color: #666;
  /*color: #4a4a4a;*/
  font-size: 26rpx;
  margin-left:66rpx;
}
.go-buy{
  padding-bottom: 18rpx;
  border-bottom:1rpx solid #e5e5e5;
}
.time-l{
  display:inline-block;
  color:#A6A6A6;
  font-size:26rpx;
}
.go-r {
    display:inline-block;
    text-align:right;
    color:#52bea6;
    font-size:26rpx;
    float:right;
}
.not{
	color: #ff7475;
}
.ml38{
  margin-left: 38px;
}
/*列表 end*/

