.header {
  width: 100%;
  position:absolute;
  top:0;
  left:0;
  right:0;
  z-index: 999;

}
.main-container {
  width:100%;
  position: absolute;
  top:97rpx;
  bottom:0;
}
.main-container swiper-item{
  display: inline-block;
}
/*tab切换css*/
.list-tab{
    height: 94rpx;
  line-height: 94rpx;
  width: 100%;
  background: white;
}
.header-wrapper{
  height: 100%;
    width: 100%;
    display: flex;
  text-align:center;
  border-bottom:1rpx solid #e8e8e8;

}
.single {
  display: inline-block;
  height: 100%;
  width: 100%;
  text-align: center;
}
.text-detail {
  font-size: 32rpx;
  height: 100%;
  color:#9097a2;
  display: inline-block;
}
.single .active {
  color: #2fa7e0;
  border-bottom: 4rpx solid #2fa7e0;
  height:92rpx;
}
.flexItem {
}
.list_active0 .view0, .list_active1 .view1{
  overflow-x: hidden;
}
.list_active0 .view0{
display: block;

}

.list_active1 .view1{
  display: block;
}

.list_active1 .view1 .none{
  display: block !important;
}

.list_active2 .view2{
  display: block;
  transition:all 0.4s ease-in-out;
    -moz-transition:all 0.4s ease-in-out;
    -webkit-transition:all 0.4s ease-in-out;
    -o-transition:all 0.4s ease-in-out;

}
.tab_con {

}
.width20 {
  width:100%;
  display: none;

}
