.header {
  width: 100%;
  border-bottom: 1rpx solid #f2f2f2;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 9999;
  background: #F1F2F6;
}

/*产品内容*/
.chanPing{
  margin: 0;
}
.top-slider {
  display: block;
  width: 100%;
  height: 40%;
  background: white;
  border: 2rpx solid #e5e5e5;
}
.slider-sec{
  width: 100%;
  height: 100%;
}
.slide-image{
  width:100%;
  height:100%;
  padding: 22rpx;
}
.list-detail {
  width: 100%;
  background: white;
  margin-bottom: 25rpx;
}
.list-detail .list-sec {
  width: 100%;
  display:inline-block;
  padding: 18rpx 0 18rpx;

}
.background{
  width:100%;
  background:#f5f5f5;
  padding:15rpx 0;
}
.image {
  display:block;
  width:150rpx;
  height:150rpx;
  border:1rpx solid #eaedf4;
  margin:8rpx auto;
  border-radius:0rpx;
  text-align:center;
  float:left;
  background: #ffffff;
}
.image image{
  display:block;
  width:150rpx;
  height:150rpx;
}
.bottom-sec {
  margin-left: 182rpx;
}
.now {
  color: #666;
  font-size: 26rpx;
  padding-bottom:6rpx;
}
.top-title{
  padding-bottom:4rpx;
}
.top-title .inline-block, .price-wrapper .inline-block{
    vertical-align: middle;
}
.top-title .title{
  width: 14%;
  color: #BBBBBB;
  font-size:26rpx;
}
.top-title .description{
  color: #BBBBBB;
  font-size: 26rpx;
}
.go-buy{
  padding-bottom: 18rpx;
  border-bottom:1rpx solid #e5e5e5;
}
.time-l{
  display:inline-block;
  color:#666;
  font-size:26rpx;
}
.go-r {
    display:inline-block;
    text-align:right;
    color:#52bea6;
    font-size:26rpx;
    float:right;
}
.not{
	color: #ff7475;
}
.w70{
    width:70%;
    display: inline-block;

}
/*回复*/
.reply{
  padding-top: 15rpx;
  border-top:1rpx solid #e5e5e5;
  color:#666;
  font-size:26rpx;
}
.w80{
    width:79%;
    display: inline-block;
}
.reply .f-time{
}
/*回复 end*/