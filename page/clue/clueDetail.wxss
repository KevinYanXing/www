.detail {
    border-left: 1rpx solid #2ea4ff;
    padding: 0px 0 10px 35px;
    margin-left: 25px;
    position: relative;
}
.detail:after {
    content: '';
    display: inline-block;
    position: absolute;
    left: -5px;
    top: 0px;
    width: 6px;
    height: 6px;
    -webkit-border-radius: 10px;
    background: #2ea4ff;
    border: 1px solid #ffffff;
}
