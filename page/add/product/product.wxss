.button{
    float: right;
    display: inline-block;
}
/*产品信息*/
.chanPing{
  margin: 25rpx;
}
cp-box{
      box-shadow: 0px 1px 3px rgba(0,0,0,0.12), 0px 1px 2px rgba(0,0,0,0.24);
}
.image{
    overflow: hidden;
    display: block;
    width: 150rpx;
    height: 150rpx;
    border: 1rpx solid #eaedf4;
    margin: 30rpx auto;
    border-radius: 0rpx;
    text-align: center;
    float: left;
}
.image view image{
    vertical-align: middle;
    width: initial;
    height: initial;
}
.cp-p{
  text-align:left;
  padding:23rpx 0 10rpx;
  margin-left:175rpx;
}
.tiaoxingMa{
  padding:10rpx 0 ;
  font-size:12px;
  color:#969696;
}
.cp-shuLang{
  padding: 0 0 10rpx;
  color:#969696;
}
.cp-gy text:nth-child(1){
  color:#333;
}
.cp-gy text:nth-child(2){
  float: right;
  color:#f53135;
}
.order-info{
	position: relative;
	font-size: 24rpx;
	color: #a9a9a9;
  padding-bottom: 10rpx;
  border-bottom:1rpx solid #e5e5e5;
}
.order-state{
	font-size:28rpx;
  position:absolute;
  right:4rpx;
  top:12rpx;
}
/*操作*/
.bottom-sec{
    padding-top: 18rpx;
    font-size: 30rpx;
    text-align:right;
    border-top:1rpx solid #e5e5e5;
}
.biao-arrow{
  display:inline-block;
  position:relative;
  top:5rpx;
}
.edit{
    padding-right:86rpx;
  display:inline-block;
  color:#0292db;
}

.delete{
    color:#f53135;
  display:inline-block;
  padding-right:20rpx;
}
/*操作 end*/
