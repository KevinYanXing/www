/* 固定页面位置 */
page{
  display:flex;
  flex-direction:column;
  height:100%;
}
/* 固定页面位置 end */

.button{
    float: right;
    display: inline-block;
}
/*产品信息*/
.chanPing{
  margin: 25rpx;
}
cp-box{
      box-shadow: 0px 2rpx 6rpx rgba(0,0,0,0.12), 0px 2rpx 4rpx rgba(0,0,0,0.24);
}
.image{
    overflow: hidden;
    display: block;
    width: 136rpx;
    height: 136rpx;
    border: 1rpx solid #eaedf4;
    margin: 30rpx auto 10rpx;
    border-radius: 0rpx;
    text-align: center;
    float: left;
}
.image view image{
    vertical-align: middle;
    width: initial;
    height: initial;
}
/*时间与系列号*/
.order-info{
  height: 34px;
  line-height: 34px;
	position: relative;
	font-size: 24rpx;
	color: #a9a9a9;
  padding-bottom: 10rpx;
  border-bottom:1rpx solid #e5e5e5;
}
.mfd-time{
  float: right;
  position:absolute;
  right:4rpx;
  top:-3rpx;
  line-height:18px;
  display:inline-block;
}
.cp-time{
  width:60rpx;
  display:inline-block;
  text-align:right;
  padding-right:12rpx;
}
.cp-p{
  text-align:left;
  padding:23rpx 0 10rpx;
  margin-left:172rpx;
}
.order-state{
  color: #333;
}
/*时间与系列号 end*/
.mingChen,.tiaoxingMa,.cp-shuLang{
  padding-bottom: 8rpx;
}
.mingChen{
  font-size:32rpx;
}
.tiaoxingMa{
  padding:10rpx 0 ;
  font-size:28rpx;
  color:#969696;
}
.cp-shuLang{
  padding: 0 0 10rpx;
  color:#969696;
}
.cp-gy text:nth-child(1){
  color:#333;
}
.cp-gy text:nth-child(2){
  float: right;
}
.cp-gy .rmb{
  
}
/*操作*/
.bottom-sec{
    padding-top: 18rpx;
    font-size: 30rpx;
    text-align:right;
    border-top:1rpx solid #e5e5e5;
}
.biao-arrow{
  display:inline-block;
  position:relative;
  top:5rpx;
}
.edit{
    padding-right:86rpx;
  display:inline-block;
  color:#0292db;
}

.delete{
    color:#f53135;
  display:inline-block;
  padding-right:20rpx;
}
/*操作 end*/
/*产品信息 end*/

