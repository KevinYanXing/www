/*搜索*/
.list-sous .non{
  text-align:center;
  line-height:1.4rem;
    height:1.4rem;
}
.list-sous .non.search{
    width: 100%;
}
.list-sous .non .icon{
  top:12rpx;
  left:-12rpx;
  float:initial;
}
.none{
  display: none !important;
}
/*搜索状态*/
.Block{
    display: block !important;
}
.list-sous{
    background:#eee;
    display:flex;
    height:75rpx;
    padding:20rpx;
}
.list-sous .search {
    width:85%;
    color:#888;
    font-size: 26rpx;
    padding:10rpx;
    border-radius:38rpx;
    background:#fff;
    border:1rpx solid #e5e5e5;
}
.list-sous.scrolled {
	background: #feb70f;
}
.list-sous .icon {
	vertical-align:top;
    position:relative;
    top:12rpx;
    left:16rpx;
    float:left;
}
.list-sous .location {
	color: #fff;
	font-size: 30rpx;
	line-height: 40rpx;
	min-width: 100rpx;
	max-width: 340rpx;
	padding: 10rpx 20rpx 10rpx 10rpx;
	border-radius: 30rpx;
	background: rgba(0,0,0,0.3);
	margin-right: 20rpx;
	vertical-align: middle;
}
.input {
    vertical-align:middle;
    margin-left:58rpx;
}
.cancel{
    color:#0091ff;
